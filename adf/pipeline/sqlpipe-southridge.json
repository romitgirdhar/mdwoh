{
	"name": "sqlpipe-southridge",
	"properties": {
		"activities": [
			{
				"name": "copydata_orders",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource"
					},
					"sink": {
						"type": "AzureBlobFSSink",
						"copyBehavior": "PreserveHierarchy"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "southridge_cs_orders",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "ADLS_southridge_orders",
						"type": "DatasetReference"
					}
				]
			}
		]
	}
}